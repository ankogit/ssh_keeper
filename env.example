# SSH Keeper Configuration
# Этот файл содержит конфигурацию для SSH Keeper

# Основные настройки
DEBUG=false
ENV=development
CONFIG_PATH=~/.ssh-keeper/config

# Настройки безопасности
APP_SIGNATURE=ssh-keeper-sig-dev

# Настройки SSH
SSH_CONFIG_PATH=~/.ssh/config

# Настройки приложения
APP_NAME=ssh-keeper
APP_VERSION=1.0.0
APP_DESCRIPTION=SSH Connection Manager

# Настройки логирования
LOG_LEVEL=info
LOG_FORMAT=text
