# SSH Keeper Production Configuration
# This file is used for production builds and releases

# Основные настройки
DEBUG=false
ENV=production
CONFIG_PATH=~/.ssh-keeper/config

# Настройки безопасности
APP_SIGNATURE=ssh-keeper-sig-prod

# Настройки SSH
SSH_CONFIG_PATH=~/.ssh/config

# Настройки приложения
APP_NAME=ssh-keeper
APP_VERSION=0.1.0
APP_DESCRIPTION=SSH Connection Manager

# Настройки логирования
LOG_LEVEL=info
LOG_FORMAT=text
